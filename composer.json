{
    "name": "dungle01/windowsazure-on-laminas",
    "description": "Forked by microsoft/windowsazure. Changed to replace zend dependency with laminas to use it with magento2. - Provides a set of PHP client libraries that make it easy to access Windows Azure tables, blobs, queues, service runtime and service management APIs.",
    "keywords": [ "php", "azure", "sdk" ],
    "license": "Apache-2.0",
    "version": "0.6.0",
    "authors": [
        {
            "name": "Azure PHP SDK",
            "email": "azurephpsdk@microsoft.com"
        }
    ],
    "require": {
        "php": "~7.4.0",
        "pear/net_url2": "^2.2",
        "pear/mail_mime": "^1.10",
        "firebase/php-jwt": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "^6.5.8",
        "laminas/laminas-mime": "^2.9.1",
        "laminas/laminas-mail": "^2.16.0",
        "microsoft/azure-storage-blob": "^1.5",
        "microsoft/azure-storage-file": "^1.2",
        "microsoft/azure-storage-table": "^1.1",
        "microsoft/azure-storage-queue": "^1.3"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "~9.5.20",
        "sebastian/phpcpd": "^2.0",
        "phploc/phploc": "^7.0.1",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6",
        "mayflower/php-codebrowser": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.8.0"
    },
    "autoload": {
        "psr-4": { "WindowsAzure\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
